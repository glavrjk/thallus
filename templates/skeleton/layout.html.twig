<!DOCTYPE html>
<html>
	<head>
		{% include "skeleton/meta.html.twig" %}
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link href="https://fonts.googleapis.com/icon?family=Material+Icons+Round" rel="stylesheet">
		<link
		rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900|Roboto+Slab:400,700"/>

		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		<script type="text/javascript" src="{{ asset('build/material-dashboard.min.js') }}"></script>
	</head>
	<body class="g-sidenav-show  bg-gray-200">
		<aside class="sidenav navbar navbar-vertical navbar-expand-xs border-0 border-radius-xl my-3 fixed-start ms-3   bg-gradient-dark" id="sidenav-main">
			<div class="sidenav-header h-25">
				<i class="fas fa-times p-3 cursor-pointer text-white opacity-5 position-absolute end-0 top-0 d-none d-xl-none" aria-hidden="true" id="iconSidenav"></i>
				<a class="navbar-brand m-0">
					<img src="{{ asset('assets/logo/logo.webp') }}" class="navbar-brand-img img-fluid mh-100" alt="main_logo">
				</a>
			</div>
			<hr class="horizontal light mt-0 mb-2">
			{% include "skeleton/menu.html.twig" %}
		</aside>
		<main
			class="main-content position-relative max-height-vh-100 h-100 border-radius-lg">
			<!-- Navbar -->
			
			<!-- End Navbar -->
			<div
				class="container-fluid py-4">
				{# Bloque #}
				{% block body %}{% endblock %}
			</div>
		</main>
	</body>
</html>
